
{
  "name": "snb-harvest",
  "version": "1.0.9",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "start:test": "cross-env EXPO_PUBLIC_ENV=test expo start",
    "start:production": "cross-env EXPO_PUBLIC_ENV=production expo start",
    "android": "expo start --android",
    "android:test": "cross-env EXPO_PUBLIC_ENV=test expo start --android",
    "android:production": "cross-env EXPO_PUBLIC_ENV=production expo start --android",
    "ios": "expo start --ios",
    "ios:test": "cross-env EXPO_PUBLIC_ENV=test expo start --ios",
    "ios:production": "cross-env EXPO_PUBLIC_ENV=production expo start --ios",
    "web": "expo start --web",
    "build:test": "eas build --profile test",
    "build:production": "eas build --profile production",
    "build:test:android": "eas build --profile test --platform android",
    "build:test:ios": "eas build --profile test --platform ios",
    "build:production:android": "eas build --profile production --platform android",
    "build:production:ios": "eas build --profile production --platform ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "dotenv": "^17.2.1",
    "expo": "~53.0.20",
    "expo-linear-gradient": "^14.1.5",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.17",
    "react": "19.0.0",
    "react-native": "0.79.5",
    "react-native-dotenv": "^3.4.11",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "cross-env": "^7.0.3"
  },
  "private": true
}
